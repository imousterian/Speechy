<h1>Users</h1>

<table class="table">
  <thead>
    <tr>
      <th>Email</th>
      <th>List of Images</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.email, user_path(user) %></td>
        <td><%= link_to 'List', summary_index_path(user) %></td>
        <td>
            <%= link_to 'Delete', user_path(user), :method => 'delete', :confirm => 'Are you sure?' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
